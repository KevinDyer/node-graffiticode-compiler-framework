{
  "name": "@graffiticode/compiler-framework",
  "version": "0.1.14",
  "repository": "https://github.com/kevindyer/node-graffiticode-compiler-framework.git",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "dependencies": {
    "aws-lambda": "^1.0.6",
    "aws-serverless-express": "^3.4.0",
    "bent": "^7.3.12",
    "express": "^4.17.1",
    "firebase-admin": "^9.12.0",
    "firebase-functions": "^3.15.7",
    "minimist": "^1.2.6",
    "morgan": "^1.10.0"
  },
  "scripts": {
    "test": "jest --testPathIgnorePatterns build",
    "test:dev": "jest --watch --testPathIgnorePatterns build",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile && copyfiles -u 1 test/assets/* build/test/",
    "posttest": "npm run lint",
    "lint": "gts lint"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build/src/**/*.js",
    "!build/src/**/*.spec.js",
    "build/src/**/*.d.ts",
    "!build/src/**/*.spec.d.ts"
  ],
  "bin": {
    "@graffiticode/compiler-framework": "./build/src/index.js",
    "compiler-framework": "./build/src/index.js"
  },
  "author": "Kevin Dyer <kevindy@google.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.84",
    "@types/aws-serverless-express": "^3.3.5",
    "@types/bent": "^7.3.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/minimist": "^1.2.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.4",
    "@types/supertest": "^2.0.11",
    "copyfiles": "^2.4.1",
    "gts": "^3.1.0",
    "jest": "^27.3.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  }
}
