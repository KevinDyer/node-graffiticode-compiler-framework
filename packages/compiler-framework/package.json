{
  "name": "@graffiticode/graffiticode-compiler-framework",
  "version": "0.1.1",
  "repository": "https://github.com/kevindyer/node-graffiticode-compiler-framework.git",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "dependencies": {
    "aws-lambda": "^1.0.6",
    "aws-serverless-express": "^3.4.0",
    "bent": "^7.3.12",
    "express": "^4.17.1",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.0",
    "minimist": "^1.2.5",
    "morgan": "^1.10.0"
  },
  "scripts": {
    "test": "jest --testPathIgnorePatterns build",
    "test:dev": "jest --watch --testPathIgnorePatterns build",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile && copyfiles -u 1 test/assets/* build/test/",
    "posttest": "npm run lint",
    "lint": "gts lint"
  },
  "files": [
    "build/src/**/*.js",
    "!build/src/**/*.spec.js",
    "build/src/**/*.d.ts",
    "!build/src/**/*.spec.d.ts"
  ],
  "bin": {
    "graffiticode-compiler-framework": "./build/src/index.js"
  },
  "author": "Kevin Dyer <kevindy@google.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.68",
    "@types/aws-serverless-express": "^3.3.3",
    "@types/bent": "^7.3.2",
    "@types/express": "^4.17.9",
    "@types/jest": "^25.2.3",
    "@types/minimist": "^1.2.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.11.2",
    "@types/supertest": "^2.0.10",
    "copyfiles": "^2.4.1",
    "gts": "^3.0.3",
    "jest": "^25.5.4",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "typescript": "^4.0.3"
  }
}
