{
  "name": "@graffiticode/graffiticode-compiler-framework",
  "version": "0.0.15",
  "repository": "https://github.com/kevindyer/node-graffiticode-compiler-framework.git",
  "main": "build/src/index.js",
  "types": "build/src/invoker.d.ts",
  "dependencies": {
    "aws-lambda": "^1.0.6",
    "aws-serverless-express": "^3.3.8",
    "bent": "^7.3.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.11.0",
    "firebase-functions": "^3.6.1",
    "minimist": "^1.2.5",
    "morgan": "^1.10.0"
  },
  "scripts": {
    "test": "jest --testPathIgnorePatterns build",
    "test:dev": "jest --watch --testPathIgnorePatterns build",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile && copyfiles -u 1 test/assets/* build/test/",
    "posttest": "npm run check"
  },
  "files": [
    "build/src/**/*.js",
    "!build/src/**/*.spec.js",
    "build/src/**/*.d.ts",
    "!build/src/**/*.spec.d.ts"
  ],
  "bin": {
    "graffiticode-compiler-framework": "./build/src/index.js"
  },
  "author": "Kevin Dyer <kevindy@google.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.51",
    "@types/aws-serverless-express": "^3.3.3",
    "@types/bent": "^7.0.2",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/minimist": "^1.2.0",
    "@types/morgan": "^1.9.0",
    "@types/node": "^10.17.21",
    "@types/supertest": "^2.0.8",
    "copyfiles": "^2.2.0",
    "gts": "^1.1.2",
    "jest": "^25.5.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  }
}
